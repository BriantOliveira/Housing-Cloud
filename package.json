{
    "name": "housing-cloud-api",
    "version": "1.0.0",
    "description": "Student Housing services",
    "main": "build/index.js",
    "scripts": {
        "start": "node build/index.js",
        "build": "tsc",
        "dist": "sh ./scripts/dist.sh",
        "start:local": "NODE_ENV=local ts-node-dev --transpile-only --respawn --watch src src/index.ts",
        "start:dev": "NODE_ENV=development ts-node-dev --transpile-only --respawn --watch src src/index.ts",
        "start:test": "NODE_ENV=test ts-node-dev --transpile-only --respawn --watch src src/index.ts",
        "start:staging": "NODE_ENV=staging ts-node-dev --transpile-only --respawn --watch src src/index.ts",
        "start:production": "NODE_ENV=production ts-node-dev --transpile-only --respawn --watch src src/index.ts",
        "test-coverage": "NODE_ENV=test nyc mocha -r ts-node/register src/tests/tests.ts --exit",
        "test": "NODE_ENV=test mocha -r ts-node/register src/tests/tests.ts --exit",
        "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
        "lint": "eslint 'src/**/*.ts' --fix",
        "prettier": "prettier --write src/**/*.ts"
    },
    "engines": {
        "node": "12.18.3",
        "npm": "6.14.6"
    },
    "author": "Elliot Briant",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": ""
    },
    "devDependencies": {
        "@types/async": "^3.2.3",
        "@types/chai": "^4.2.12",
        "@types/cors": "^2.8.6",
        "@types/express": "^4.17.6",
        "@types/jsonwebtoken": "^8.5.0",
        "@types/mocha": "^8.0.3",
        "@types/multer-s3": "^2.7.7",
        "@types/node": "^8.10.66",
        "@types/prettyjson": "^0.0.30",
        "@types/uuid": "^8.3.0",
        "@typescript-eslint/eslint-plugin": "^3.10.1",
        "@typescript-eslint/parser": "^3.10.1",
        "chai": "^4.2.0",
        "chai-http": "^4.3.0",
        "eslint": "^7.23.0",
        "eslint-config-airbnb": "^18.2.0",
        "eslint-config-prettier": "^8.1.0",
        "eslint-plugin-import": "^2.24.0",
        "eslint-plugin-jest": "^24.4.0",
        "eslint-plugin-jsx-a11y": "^6.4.1",
        "eslint-plugin-prettier": "^3.3.1",
        "eslint-plugin-react": "^7.24.0",
        "eslint-plugin-react-hooks": "^4.2.0",
        "fp-ts": "^2.6.6",
        "jest": "^27.2.5",
        "mocha": "^9.1.2",
        "monocle-ts": "^2.2.0",
        "newtype-ts": "^0.3.4",
        "nyc": "^15.1.0",
        "prettier": "^2.2.1",
        "trim-newlines": "^4.0.2",
        "ts-node": "^3.3.0",
        "ts-node-dev": "^1.1.8",
        "typescript": "^4.5.5"
    },
    "dependencies": {
        "axios": "^0.27.2",
        "body-parser": "^1.19.0",
        "cors": "^2.8.5",
        "dotenv": "^8.2.0",
        "express": "^4.18.1",
        "express-sanitizer": "^1.0.6",
        "express-validator": "^6.8.0",
        "io-ts": "^2.2.6",
        "io-ts-types": "^0.5.7",
        "jsonwebtoken": "^8.5.1",
        "multer-s3": "^2.10.0",
        "path": "^0.12.7",
        "pg": "^7.3.0",
        "prettyjson": "^1.2.1",
        "typeorm": "^0.2.38",
        "uuid": "^8.3.2",
        "winston": "^3.2.1"
    }
}
